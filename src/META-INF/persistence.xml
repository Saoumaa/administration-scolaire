<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="ScolairePU" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <class>entites.Eleve</class>
    <class>entites.AnneeScolaire</class>
    <class>entites.Trimestre</class>
    <class>entites.Serie</class>
    <class>entites.BulletinNote</class>
    <class>entites.Promotion</class>
    <class>entites.Enseigner</class>
    <class>entites.Enseignant</class>
    <class>entites.Activite</class>
    <class>entites.DetailsBulletin</class>
    <class>entites.EleveGp</class>
    <class>entites.Coefficient</class>
    <class>entites.GroupPedag</class>
    <class>entites.Cycles</class>
    <class>entites.Groupe</class>
    <class>entites.Matiere</class>
    <class>entites.Regime</class>
    <class>entites.Users</class>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:postgresql://localhost:5432/scolaire2"/>
      <property name="javax.persistence.jdbc.user" value="postgres"/>
      <property name="javax.persistence.jdbc.driver" value="org.postgresql.Driver"/>
      <property name="javax.persistence.jdbc.password" value="root"/>
    </properties>
  </persistence-unit>
</persistence>
